#: bayesml.poisson:1 of
msgid "The Poisson distribution with the gamma prior distribution."
msgstr ""

#: bayesml.poisson:3 of
msgid "The stochastic data generative model is as follows:"
msgstr ""

#: bayesml.poisson:5 of
#, python-brace-format
msgid ":math:`x \\in \\mathbb{N}`: a data point"
msgstr ""

#: bayesml.poisson:6 of
#, python-brace-format
msgid ":math:`\\lambda \\in \\mathbb{R}_{>0}`: a parameter"
msgstr ""

#: bayesml.poisson:8 of
msgid ""
"p(x | \\lambda) = \\mathrm{Po}(x|\\lambda) = \\frac{ \\lambda^{x} "
"}{x!}\\exp \\{ -\\lambda \\}.\n"
"\n"
msgstr ""

#: bayesml.poisson:11 of
msgid "The prior distribution is as follows:"
msgstr ""

#: bayesml.poisson:13 of
#, python-brace-format
msgid ":math:`\\alpha_0 \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.poisson:14 of
#, python-brace-format
msgid ":math:`\\beta_0 \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.poisson:15 of
#, python-brace-format
msgid ""
":math:`\\Gamma(\\cdot): \\mathbb{R}_{>0} \\to \\mathbb{R}_{>0}`: the "
"Gamma function"
msgstr ""

#: bayesml.poisson:17 of
msgid ""
"p(\\lambda) = \\mathrm{Gam}(\\lambda|\\alpha_0,\\beta_0) = "
"\\frac{\\beta_0^{\\alpha_0}}{\\Gamma (\\alpha_0)} \\lambda^{\\alpha_0 - "
"1} \\exp \\{ -\\beta_0 \\lambda \\},\n"
"\n"
msgstr ""

#: bayesml.poisson:20 of
#, python-brace-format
msgid ""
"\\mathbb{E}[\\lambda] &= \\frac{\\alpha_0}{\\beta_0}, \\\\\n"
"\\mathbb{V}[\\lambda] &= \\frac{\\alpha_0}{\\beta_0^2}.\n"
"\n"
msgstr ""

#: bayesml.poisson:24 of
msgid "The posterior distribution is as follows:"
msgstr ""

#: bayesml.poisson:26 of
#, python-brace-format
msgid ":math:`x^n = (x_1, x_2, \\dots , x_n) \\in \\mathbb{N}^n`: given data"
msgstr ""

#: bayesml.poisson:27 of
#, python-brace-format
msgid ":math:`\\alpha_n \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.poisson:28 of
#, python-brace-format
msgid ":math:`\\beta_n \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.poisson:30 of
msgid ""
"p(\\lambda | x^n) = \\mathrm{Gam}(\\lambda|\\alpha_n,\\beta_n) = "
"\\frac{\\beta_n^{\\alpha_n}}{\\Gamma (\\alpha_n)} \\lambda^{\\alpha_n - "
"1} \\exp \\{ -\\beta_n \\lambda \\},\n"
"\n"
msgstr ""

#: bayesml.poisson:33 of
#, python-brace-format
msgid ""
"\\mathbb{E}[\\lambda | x^n] &= \\frac{\\alpha_n}{\\beta_n}, \\\\\n"
"\\mathbb{V}[\\lambda | x^n] &= \\frac{\\alpha_n}{\\beta_n^2},\n"
"\n"
msgstr ""

#: bayesml.poisson:37 of
msgid "where the updating rule of the hyperparameters is"
msgstr ""

#: bayesml.poisson:39 of
#, python-brace-format
msgid ""
"\\alpha_n &= \\alpha_0 + \\sum_{i=1}^n x_i,\\\\\n"
"\\beta_n &= \\beta_0 + n.\n"
"\n"
msgstr ""

#: bayesml.poisson:43 of
msgid "The predictive distribution is as follows:"
msgstr ""

#: bayesml.poisson:45 of
#, python-brace-format
msgid ":math:`x_{n+1} \\in \\mathbb{N}`: a new data point"
msgstr ""

#: bayesml.poisson:46 of
#, python-brace-format
msgid ""
":math:`\\theta_\\mathrm{p} \\in \\mathbb{R}_{>0}, "
"0<\\theta_\\mathrm{p}<1`: the hyperparameter of the posterior"
msgstr ""

#: bayesml.poisson:47 of
#, python-brace-format
msgid ""
":math:`r_\\mathrm{p} \\in \\mathbb{N}`: the hyperparameter of the "
"posterior"
msgstr ""

#: bayesml.poisson:49 of
msgid ""
"\\mathrm{NB}(x_{n+1}|r_\\mathrm{p},\\theta_\\mathrm{p}) = "
"\\frac{\\Gamma(x_{n+1}+r_\\mathrm{p})}{\\Gamma(x_{n+1}+1)\\Gamma(r_\\mathrm{p})}\\theta_\\mathrm{p}^{x_{n+1}}(1-\\theta_\\mathrm{p})^{r_\\mathrm{p}},"
"\n"
"\n"
msgstr ""

#: bayesml.poisson:52 of
msgid ""
"\\mathbb{E}[x_{n+1} | x^n] &= \\frac{r_\\mathrm{p} \\theta_\\mathrm{p}}{1"
" - \\theta_\\mathrm{p}}, \\\\\n"
"\\mathbb{V}[x_{n+1} | x^n] &= \\frac{r_\\mathrm{p} "
"\\theta_\\mathrm{p}}{(1 - \\theta_\\mathrm{p})^2},\n"
"\n"
msgstr ""

#: bayesml.poisson:56 of
msgid ""
"where the parameters are obtained from the hyperparameters of the "
"posterior distribution as follows:"
msgstr ""

#: bayesml.poisson:58 of
#, python-brace-format
msgid ""
"&r_\\mathrm{p}=\\alpha_n, \\\\\n"
"&\\theta_\\mathrm{p} = \\frac{1}{\\beta_n + 1}.\n"
"\n"
msgstr ""

