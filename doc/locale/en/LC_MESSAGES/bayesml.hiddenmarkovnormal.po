#: ../../bayesml.hiddenmarkovnormal.rst:2
msgid "bayesml.hiddenmarkovnormal package"
msgstr ""

#: ../../bayesml.hiddenmarkovnormal.rst:7
msgid "Module contents"
msgstr ""

#: bayesml.hiddenmarkovnormal:1 of
msgid ""
"The hidden Markov model with the Gauss-Wishart prior distribution and the"
" Dirichlet prior distribution."
msgstr ""

#: bayesml.hiddenmarkovnormal:3 of
msgid "The stochastic data generative model is as follows:"
msgstr ""

#: bayesml.hiddenmarkovnormal:5 of
#, python-brace-format
msgid ":math:`K \\in \\mathbb{N}`: number of latent classes"
msgstr ""

#: bayesml.hiddenmarkovnormal:6 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{z} \\in \\{ 0, 1 \\}^K`: a one-hot vector "
"representing the latent class (latent variable)"
msgstr ""

#: bayesml.hiddenmarkovnormal:7 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{\\pi} \\in [0, 1]^K`: a parameter for latent classes,"
" (:math:`\\sum_{k=1}^K \\pi_k=1`)"
msgstr ""

#: bayesml.hiddenmarkovnormal:8 of
#, python-brace-format
msgid ""
":math:`a_{j,k} \\in [0,1]` : transition probability to latent state k "
"under latent state j"
msgstr ""

#: bayesml.hiddenmarkovnormal:9 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{a}_j = [a_{j,1}, a_{j,2}, \\dots , a_{j,K}]\\in "
"[0,1]^K`, a vector of the transition probability (:math:`\\sum_{k=1}^K "
"a_{j,k}=1`)"
msgstr ""

#: bayesml.hiddenmarkovnormal:10 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{A}=(a_{j,k})_{1\\leq j,k\\leq K} \\in [0, "
"1]^{K\\times K}`: a matrix of the transition probability"
msgstr ""

#: bayesml.hiddenmarkovnormal:11 of
#, python-brace-format
msgid ":math:`D \\in \\mathbb{N}`: a dimension of data"
msgstr ""

#: bayesml.hiddenmarkovnormal:12 of
#, python-brace-format
msgid ":math:`\\boldsymbol{x} \\in \\mathbb{R}^D`: a data point"
msgstr ""

#: bayesml.hiddenmarkovnormal:13 of
#, python-brace-format
msgid ":math:`\\boldsymbol{\\mu}_k \\in \\mathbb{R}^D`: a parameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:14 of
msgid ":math:`\\boldsymbol{\\mu} = \\{ \\boldsymbol{\\mu}_k \\}_{k=1}^K`"
msgstr ""

#: bayesml.hiddenmarkovnormal:15 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{\\Lambda}_k \\in \\mathbb{R}^{D\\times D}` : a "
"parameter (a positive definite matrix)"
msgstr ""

#: bayesml.hiddenmarkovnormal:16 of
msgid ":math:`\\boldsymbol{\\Lambda} = \\{ \\boldsymbol{\\Lambda}_k \\}_{k=1}^K`"
msgstr ""

#: bayesml.hiddenmarkovnormal:17 of
#, python-brace-format
msgid ""
":math:`| \\boldsymbol{\\Lambda}_k | \\in \\mathbb{R}`: the determinant of"
" :math:`\\boldsymbol{\\Lambda}_k`"
msgstr ""

#: bayesml.hiddenmarkovnormal:19 of
msgid ""
"p(\\boldsymbol{z}_{1} | \\boldsymbol{\\pi}) &= "
"\\mathrm{Cat}(\\boldsymbol{z}_{1}|\\boldsymbol{\\pi}) = \\prod_{k=1}^K "
"\\pi_k^{z_{1,k}},\\\\\n"
"p(\\boldsymbol{z}_{n} |\\boldsymbol{z}_{n-1} ,\\boldsymbol{A}) &= "
"\\prod_{k=1}^K \\prod_{j=1}^K a_{j,k}^{z_{n-1,j}z_{n,k}},\\\\\n"
"p(\\boldsymbol{x}_{n} | \\boldsymbol{\\mu}, \\boldsymbol{\\Lambda}, "
"\\boldsymbol{z}_{n}) &= \\prod_{k=1}^K "
"\\mathcal{N}(\\boldsymbol{x}|\\boldsymbol{\\mu}_k,\\boldsymbol{\\Lambda}_k^{-1})^{z_{n,k}}"
" \\\\\n"
"&= \\prod_{k=1}^K \\left( \\frac{| \\boldsymbol{\\Lambda}_{k} "
"|^{1/2}}{(2\\pi)^{D/2}} \\exp \\left\\{ "
"-\\frac{1}{2}(\\boldsymbol{x}-\\boldsymbol{\\mu}_{k})^\\top "
"\\boldsymbol{\\Lambda}_{k} (\\boldsymbol{x}-\\boldsymbol{\\mu}_{k}) "
"\\right\\} \\right)^{z_{n,k}},\n"
"\n"
msgstr ""

#: bayesml.hiddenmarkovnormal:25 of
msgid "The prior distribution is as follows:"
msgstr ""

#: bayesml.hiddenmarkovnormal:27 of
#, python-brace-format
msgid ":math:`\\boldsymbol{m}_0 \\in \\mathbb{R}^{D}`: a hyperparameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:28 of
#, python-brace-format
msgid ":math:`\\kappa_0 \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:29 of
#, python-brace-format
msgid ":math:`\\nu_0 \\in \\mathbb{R}`: a hyperparameter (:math:`\\nu_0 > D-1`)"
msgstr ""

#: bayesml.hiddenmarkovnormal:30 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{W}_0 \\in \\mathbb{R}^{D\\times D}`: a hyperparameter"
" (a positive definite matrix)"
msgstr ""

#: bayesml.hiddenmarkovnormal:31 of
#, python-brace-format
msgid ":math:`\\boldsymbol{\\eta}_0 \\in \\mathbb{R}_{> 0}^K`: a hyperparameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:32 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{\\zeta}_{0,j} \\in \\mathbb{R}_{> 0}^K`: a "
"hyperparameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:33 of
#, python-brace-format
msgid ":math:`\\mathrm{Tr} \\{ \\cdot \\}`: a trace of a matrix"
msgstr ""

#: bayesml.hiddenmarkovnormal:34 of
msgid ":math:`\\Gamma (\\cdot)`: the gamma function"
msgstr ""

#: bayesml.hiddenmarkovnormal:36 of
msgid ""
"p(\\boldsymbol{\\mu},\\boldsymbol{\\Lambda},\\boldsymbol{\\pi},\\boldsymbol{A})"
" &= \\left\\{ \\prod_{k=1}^K "
"\\mathcal{N}(\\boldsymbol{\\mu}_k|\\boldsymbol{m}_0,(\\kappa_0 "
"\\boldsymbol{\\Lambda}_k)^{-1})\\mathcal{W}(\\boldsymbol{\\Lambda}_k|\\boldsymbol{W}_0,"
" \\nu_0) \\right\\} "
"\\mathrm{Dir}(\\boldsymbol{\\pi}|\\boldsymbol{\\eta}_0) "
"\\prod_{j=1}^{K}\\mathrm{Dir}(\\boldsymbol{a}_{j}|\\boldsymbol{\\zeta}_{0,j}),"
" \\\\\n"
"&= \\Biggl[ \\prod_{k=1}^K \\left( \\frac{\\kappa_0}{2\\pi} "
"\\right)^{D/2} |\\boldsymbol{\\Lambda}_k|^{1/2} \\exp \\left\\{ "
"-\\frac{\\kappa_0}{2}(\\boldsymbol{\\mu}_k -\\boldsymbol{m}_0)^\\top "
"\\boldsymbol{\\Lambda}_k (\\boldsymbol{\\mu}_k - \\boldsymbol{m}_0) "
"\\right\\} \\\\\n"
"&\\qquad \\times B(\\boldsymbol{W}_0, \\nu_0) | \\boldsymbol{\\Lambda}_k "
"|^{(\\nu_0 - D - 1) / 2} \\exp \\left\\{ -\\frac{1}{2} \\mathrm{Tr} \\{ "
"\\boldsymbol{W}_0^{-1} \\boldsymbol{\\Lambda}_k \\} \\right\\}\\biggl] "
"\\\\\n"
"&\\qquad \\times \\Biggl[ "
"\\prod_{k=1}^KC(\\boldsymbol{\\eta}_0)\\pi_k^{\\eta_{0,k}-1}\\biggl]\\\\"
"\n"
"&\\qquad \\times "
"\\biggl[\\prod_{j=1}^KC(\\boldsymbol{\\zeta}_{0,j})\\prod_{k=1}^K "
"a_{j,k}^{\\zeta_{0,j,k}-1}\\Biggr],\\\\\n"
"\n"
msgstr ""

#: bayesml.hiddenmarkovnormal:43 of
#, python-brace-format
msgid ""
"where :math:`B(\\boldsymbol{W}_0, \\nu_0)` and "
":math:`C(\\boldsymbol{\\eta}_0)` are defined as follows:"
msgstr ""

#: bayesml.hiddenmarkovnormal:45 of
msgid ""
"B(\\boldsymbol{W}_0, \\nu_0) &= | \\boldsymbol{W}_0 |^{-\\nu_0 / 2} "
"\\left( 2^{\\nu_0 D / 2} \\pi^{D(D-1)/4} \\prod_{i=1}^D \\Gamma \\left( "
"\\frac{\\nu_0 + 1 - i}{2} \\right) \\right)^{-1}, \\\\\n"
"C(\\boldsymbol{\\eta}_0) &= \\frac{\\Gamma(\\sum_{k=1}^K "
"\\eta_{0,k})}{\\Gamma(\\eta_{0,1})\\cdots\\Gamma(\\eta_{0,K})},\\\\\n"
"C(\\boldsymbol{\\zeta}_{0,j}) &= \\frac{\\Gamma(\\sum_{k=1}^K "
"\\zeta_{0,j,k})}{\\Gamma(\\zeta_{0,j,1})\\cdots\\Gamma(\\zeta_{0,j,K})}."
" \n"
"\n"
msgstr ""

#: bayesml.hiddenmarkovnormal:50 of
msgid ""
"The apporoximate posterior distribution in the :math:`t`-th iteration of "
"a variational Bayesian method is as follows:"
msgstr ""

#: bayesml.hiddenmarkovnormal:52 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{x}^n = (\\boldsymbol{x}_1, \\boldsymbol{x}_2, \\dots "
", \\boldsymbol{x}_n) \\in \\mathbb{R}^{D \\times n}`: given data"
msgstr ""

#: bayesml.hiddenmarkovnormal:53 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{z}^n = (\\boldsymbol{z}_1, \\boldsymbol{z}_2, \\dots "
", \\boldsymbol{z}_n) \\in \\{ 0, 1 \\}^{K \\times n}`: latent classes of "
"given data"
msgstr ""

#: bayesml.hiddenmarkovnormal:54 of
#, python-brace-format
msgid ":math:`\\boldsymbol{m}_{n,k}^{(t)} \\in \\mathbb{R}^{D}`: a hyperparameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:55 of
#, python-brace-format
msgid ":math:`\\kappa_{n,k}^{(t)} \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:56 of
#, python-brace-format
msgid ""
":math:`\\nu_{n,k}^{(t)} \\in \\mathbb{R}`: a hyperparameter "
":math:`(\\nu_n > D-1)`"
msgstr ""

#: bayesml.hiddenmarkovnormal:57 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{W}_{n,k}^{(t)} \\in \\mathbb{R}^{D\\times D}`: a "
"hyperparameter (a positive definite matrix)"
msgstr ""

#: bayesml.hiddenmarkovnormal:58 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{\\eta}_n^{(t)} \\in \\mathbb{R}_{> 0}^K`: a "
"hyperparameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:59 of
#, python-brace-format
msgid ""
":math:`\\boldsymbol{\\zeta}_{n,j}^{(t)} \\in \\mathbb{R}_{> 0}^K`: a "
"hyperparameter"
msgstr ""

#: bayesml.hiddenmarkovnormal:61 of
msgid ""
"&q(\\boldsymbol{z}^n, "
"\\boldsymbol{\\mu},\\boldsymbol{\\Lambda},\\boldsymbol{\\pi},\\boldsymbol{A})"
" \\nonumber \\\\\n"
"&= q^{(t)}(\\boldsymbol{z}^n) \\left\\{ \\prod_{k=1}^K "
"\\mathcal{N}(\\boldsymbol{\\mu}_k|\\boldsymbol{m}_{n,k}^{(t)},(\\kappa_{n,k}^{(t)}"
" "
"\\boldsymbol{\\Lambda}_k)^{-1})\\mathcal{W}(\\boldsymbol{\\Lambda}_k|\\boldsymbol{W}_{n,k}^{(t)},"
" \\nu_{n,k}^{(t)}) \\right\\} "
"\\mathrm{Dir}(\\boldsymbol{\\pi}|\\boldsymbol{\\eta}_n^{(t)})\\left\\{\\prod_{j=1}^K\\mathrm{Dir}(\\boldsymbol{a}_j|\\boldsymbol{\\zeta}_{n,j}^{(t)})\\right\\},"
" \\\\\n"
"&= q^{(t)}(\\boldsymbol{z}^n) \\Biggl[ \\prod_{k=1}^K \\left( "
"\\frac{\\kappa_{n,k}^{(t)}}{2\\pi} \\right)^{D/2} "
"|\\boldsymbol{\\Lambda}_k|^{1/2} \\exp \\left\\{ "
"-\\frac{\\kappa_{n,k}^{(t)}}{2}(\\boldsymbol{\\mu}_k "
"-\\boldsymbol{m}_{n,k}^{(t)})^\\top \\boldsymbol{\\Lambda}_k "
"(\\boldsymbol{\\mu}_k - \\boldsymbol{m}_{n,k}^{(t)}) \\right\\} \\\\\n"
"&\\qquad \\times B(\\boldsymbol{W}_{n,k}^{(t)}, \\nu_{n,k}^{(t)}) | "
"\\boldsymbol{\\Lambda}_k |^{(\\nu_{n,k}^{(t)} - D - 1) / 2} \\exp "
"\\left\\{ -\\frac{1}{2} \\mathrm{Tr} \\{ ( \\boldsymbol{W}_{n,k}^{(t)} "
")^{-1} \\boldsymbol{\\Lambda}_k \\} \\right\\} \\Biggr] \\\\\n"
"&\\qquad \\times C(\\boldsymbol{\\eta}_n^{(t)})\\prod_{k=1}^K "
"\\pi_k^{\\eta_{n,k}^{(t)}-1}\\left[\\prod_{j=1}^K "
"C(\\boldsymbol{\\zeta}_{n,j}^{(t)})\\prod_{k=1}^K "
"a_{j,k}^{\\zeta_{n,j,k}^{(t)}-1}\\right],\\\\\n"
"\n"
msgstr ""

#: bayesml.hiddenmarkovnormal:68 of
msgid "where the updating rule of the hyperparameters is as follows."
msgstr ""

#: bayesml.hiddenmarkovnormal:70 of
msgid ""
"N_k^{(t)} &= \\sum_{i=1}^n \\gamma^{(t)}_{i,k}, \\\\\n"
"M_{j,k}^{(t)} &= \\sum_{i=2}^n \\xi^{(t)}_{i,j,k},\\\\\n"
"\\bar{\\boldsymbol{x}}_k^{(t)} &= \\frac{1}{N_k^{(t)}} \\sum_{i=1}^n "
"\\gamma^{(t)}_{i,k} \\boldsymbol{x}_i, \\\\\n"
"S_k^{(t)} &= \\frac{1}{N_k^{(t)}}\\sum_{i=1}^n \\gamma^{(t)}_{i,k} "
"(x_i-\\bar{\\boldsymbol{x}}_k^{(t)})(x_i-\\bar{\\boldsymbol{x}}_k^{(t)})^{\\top},\\\\"
"\n"
"\\boldsymbol{m}_{n,k}^{(t+1)} &= \\frac{\\kappa_0\\boldsymbol{\\mu}_0 + "
"N_k^{(t)} \\bar{\\boldsymbol{x}}_k^{(t)}}{\\kappa_0 + N_k^{(t)}}, \\\\\n"
"\\kappa_{n,k}^{(t+1)} &= \\kappa_0 + N_k^{(t)}, \\\\\n"
"(\\boldsymbol{W}_{n,k}^{(t+1)})^{-1} &= \\boldsymbol{W}_0^{-1} + "
"N_k^{(t)}S_k^{(t)} + \\frac{\\kappa_0 N_k^{(t)}}{\\kappa_0 + "
"N_k^{(t)}}(\\bar{\\boldsymbol{x}}_k^{(t)}-\\boldsymbol{\\mu}_0)(\\bar{\\boldsymbol{x}}_k^{(t)}-\\boldsymbol{\\mu}_0)^\\top,"
" \\\\\n"
"\\nu_{n,k}^{(t+1)} &= \\nu_0 + N_k^{(t)},\\\\\n"
"\\eta_{n,k}^{(t+1)} &= \\eta_{0,k} + \\gamma^{(t)}_{1,k}, \\\\\n"
"\\zeta_{n,j,k}^{(t+1)} &= \\zeta_{0,j,k}+M_{j,k}^{(t)}.\n"
"\n"
msgstr ""

#: bayesml.hiddenmarkovnormal:82 of
#, python-brace-format
msgid ""
"The approximate posterior distribution of the latent variable "
":math:`q^{(t+1)}(z^n)` is calculated by the forward-backward algorithm as"
" follows."
msgstr ""

#: bayesml.hiddenmarkovnormal:84 of
msgid ""
"\\ln \\rho_{i,k}^{(t+1)} &= \\frac{1}{2} \\Biggl[\\, \\sum_{d=1}^D \\psi "
"\\left( \\frac{\\nu_{n,k}^{(t+1)} + 1 - d}{2} \\right) + D \\ln 2 + \\ln "
"| \\boldsymbol{W}_{n,k}^{(t+1)} | \\notag \\\\\n"
"&\\qquad - D \\ln (2 \\pi ) - \\frac{D}{\\kappa_{n,k}^{(t+1)}} - "
"\\nu_{n,k}^{(t+1)} (\\boldsymbol{x}_i - "
"\\boldsymbol{m}_{n,k}^{(t+1)})^\\top \\boldsymbol{W}_{n,k}^{(t+1)} "
"(\\boldsymbol{x}_i - \\boldsymbol{m}_{n,k}^{(t+1)}) \\Biggr], \\\\\n"
"\\ln \\tilde{\\pi}_k^{(t+1)} &= \\psi (\\eta_{n,k}^{(t+1)}) - \\psi "
"\\left( \\textstyle \\sum_{k=1}^K \\eta_{n,k}^{(t+1)} \\right) \\\\\n"
"\\ln \\tilde{a}_{j,k}^{(t+1)} &= \\psi (\\zeta_{n,j,k}^{(t+1)}) - \\psi "
"\\left( \\textstyle \\sum_{k=1}^K \\zeta_{n,j,k}^{(t+1)} \\right) \\\\\n"
"\\alpha^{(t+1)} (\\boldsymbol{z}_i) &\\propto\n"
"\\begin{cases}\n"
"\\prod_{k=1}^{K} \\left( \\rho_{i,k}^{(t+1)}\\right)^{z_{i,k}} "
"\\sum_{\\boldsymbol{z}_{i-1}} "
"\\left[\\prod_{k=1}^{K}\\prod_{j=1}^{K}\\left(\\tilde{a}^{(t+1)}_{j,k}\\right)^{z_{i-1,j}z_{i,k}}\\alpha^{(t+1)}(\\boldsymbol{z}_{i-1})\\right]"
" & (i>1)\\\\\n"
"\\prod_{k=1}^{K}\\left( \\rho_{1,k}^{(t+1)} \\tilde{\\pi}_k^{(t+1)} "
"\\right)^{z_{1,k}} & (i=1)\n"
"\\end{cases} \\\\\n"
"\\beta^{(t+1)} (\\boldsymbol{z}_i) &\\propto\n"
"\\begin{cases}\n"
"\\sum_{\\boldsymbol{z}_{i+1}} \\left[ \\prod_{k=1}^{K} \\left( "
"\\rho_{i+1,k}^{(t+1)}\\right)^{z_{i+1,k}} "
"\\prod_{k=1}^{K}\\prod_{j=1}^{K}\\left(\\tilde{a}^{(t+1)}_{j,k}\\right)^{z_{i,j}z_{i+1,k}}\\beta^{(t+1)}(\\boldsymbol{z}_{i+1})\\right]"
" & (i<n)\\\\\n"
"1 & (i=n)\n"
"\\end{cases} \\\\\n"
"q^{(t+1)}(\\boldsymbol{z}_i) &\\propto "
"\\alpha^{(t+1)}(\\boldsymbol{z}_i)\\beta^{(t+1)}(\\boldsymbol{z}_i) \\\\"
"\n"
"\\gamma^{(t+1)}_{i,k} &= \\sum_{\\boldsymbol{z}_i} "
"q^{(t+1)}(\\boldsymbol{z}_i) z_{i,k}\\\\\n"
"q^{(t+1)}(\\boldsymbol{z}_{i-1}, \\boldsymbol{z}_{i}) &\\propto "
"\\alpha^{(t+1)}(\\boldsymbol{z}_{i-1}) \\prod_{k=1}^{K} \\left( "
"\\rho_{i,k}^{(t+1)}\\right)^{z_{i,k}} "
"\\prod_{k=1}^{K}\\prod_{j=1}^{K}\\left(\\tilde{a}^{(t+1)}_{j,k}\\right)^{z_{i-1,j}z_{i,k}}"
" \\beta^{(t+1)}(\\boldsymbol{z}_i) \\\\\n"
"\\xi^{(t+1)}_{i,j,k} &= \\sum_{\\boldsymbol{z}_{i-1}} "
"\\sum_{\\boldsymbol{z}_i} q^{(t+1)}(\\boldsymbol{z}_{i-1}, "
"\\boldsymbol{z}_{i}) z_{i-1,j} z_{i,k}\n"
"\n"
msgstr ""

#: bayesml.hiddenmarkovnormal:104 of
msgid "The approximate predictive distribution is as follows:"
msgstr ""

#: bayesml.hiddenmarkovnormal:106 of
#, python-brace-format
msgid ":math:`\\boldsymbol{x}_{n+1} \\in \\mathbb{R}^D`: a new data point"
msgstr ""

#: bayesml.hiddenmarkovnormal:107 of
msgid ""
":math:`(a_{\\mathrm{p},j,k})_{1\\leq j,k\\leq K} \\in [0, 1]^{K\\times "
"K}`: the parameters of the predictive transition probability of latent "
"classes, (:math:`\\sum_{k=1}^K a_{\\mathrm{p},j,k}=1`)"
msgstr ""

#: bayesml.hiddenmarkovnormal:108 of
msgid ""
":math:`\\boldsymbol{\\mu}_{\\mathrm{p},k} \\in \\mathbb{R}^D`: the "
"parameter of the predictive distribution"
msgstr ""

#: bayesml.hiddenmarkovnormal:109 of
msgid ""
":math:`\\boldsymbol{\\Lambda}_{\\mathrm{p},k} \\in \\mathbb{R}^{D \\times"
" D}`: the parameter of the predictive distribution (a positive definite "
"matrix)"
msgstr ""

#: bayesml.hiddenmarkovnormal:110 of
msgid ""
":math:`\\nu_{\\mathrm{p},k} \\in \\mathbb{R}_{>0}`: the parameter of the "
"predictive distribution"
msgstr ""

#: bayesml.hiddenmarkovnormal:112 of
msgid ""
"&p(x_{n+1}|x^n) \\\\\n"
"&\\approx \\sum_{k=1}^K \\left( \\sum_{j=1}^K \\gamma_{n,j}^{(t)} "
"a_{\\mathrm{p},j,k} \\right) "
"\\mathrm{St}(x_{n+1}|\\boldsymbol{\\mu}_{\\mathrm{p},k},\\boldsymbol{\\Lambda}_{\\mathrm{p},k},"
" \\nu_{\\mathrm{p},k}) \\\\\n"
"&= \\sum_{k=1}^K \\left( \\sum_{j=1}^K \\gamma_{n,j}^{(t)} "
"a_{\\mathrm{p},j,k} \\right)\\Biggl[ \\frac{\\Gamma (\\nu_{\\mathrm{p},k}"
" / 2 + D / 2)}{\\Gamma (\\nu_{\\mathrm{p},k} / 2)} "
"\\frac{|\\boldsymbol{\\Lambda}_{\\mathrm{p},k}|^{1/2}}{(\\nu_{\\mathrm{p},k}"
" \\pi)^{D/2}} \\nonumber \\\\\n"
"&\\qquad \\qquad \\qquad \\qquad \\qquad \\times \\left( 1 + "
"\\frac{1}{\\nu_{\\mathrm{p},k}} (\\boldsymbol{x}_{n+1} - "
"\\boldsymbol{\\mu}_{\\mathrm{p},k})^\\top "
"\\boldsymbol{\\Lambda}_{\\mathrm{p},k} (\\boldsymbol{x}_{n+1} - "
"\\boldsymbol{\\mu}_{\\mathrm{p},k}) \\right)^{-\\nu_{\\mathrm{p},k}/2 - "
"D/2} \\Biggr],\n"
"\n"
msgstr ""

#: bayesml.hiddenmarkovnormal:118 of
msgid ""
"where the parameters are obtained from the hyperparameters of the "
"predictive distribution as follows:"
msgstr ""

#: bayesml.hiddenmarkovnormal:120 of
msgid ""
"a_{\\mathrm{p},j,k} &= \\frac{\\zeta_{n,j,k}^{(t)}}{\\sum_{k=1}^K "
"\\zeta_{n,j,k}^{(t)}}, \\\\\n"
"\\boldsymbol{\\mu}_{\\mathrm{p},k} &= \\boldsymbol{m}_{n,k}^{(t)}, \\\\\n"
"\\boldsymbol{\\Lambda}_{\\mathrm{p},k} &= \\frac{\\kappa_{n,k}^{(t)} "
"(\\nu_{n,k}^{(t)} - D + 1)}{\\kappa_{n,k}^{(t)} + 1} "
"\\boldsymbol{W}_{n,k}^{(t)}, \\\\\n"
"\\nu_{\\mathrm{p},k} &= \\nu_{n,k}^{(t)} - D + 1.\n"
"\n"
msgstr ""

