#: bayesml.bernoulli:1 of
msgid "The Bernoulli distribution with the beta prior distribution."
msgstr ""

#: bayesml.bernoulli:3 of
msgid "The stochastic data generative model is as follows:"
msgstr ""

#: bayesml.bernoulli:5 of
#, python-brace-format
msgid ":math:`x \\in \\{ 0, 1\\}`: a data point"
msgstr ""

#: bayesml.bernoulli:6 of
msgid ":math:`\\theta \\in [0, 1]`: a parameter"
msgstr ""

#: bayesml.bernoulli:8 of
#, python-brace-format
msgid ""
"p(x | \\theta) = \\mathrm{Bern}(x|\\theta) = \\theta^x (1-\\theta)^{1-x}."
"\n"
"\n"
msgstr ""

#: bayesml.bernoulli:11 of
#, python-brace-format
msgid ""
"\\mathbb{E}[x | \\theta] &= \\theta, \\\\\n"
"\\mathbb{V}[x | \\theta] &= \\theta (1 - \\theta).\n"
"\n"
msgstr ""

#: bayesml.bernoulli:15 of
msgid "The prior distribution is as follows:"
msgstr ""

#: bayesml.bernoulli:17 of
#, python-brace-format
msgid ":math:`\\alpha_0 \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.bernoulli:18 of
#, python-brace-format
msgid ":math:`\\beta_0 \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.bernoulli:19 of
#, python-brace-format
msgid ""
":math:`B(\\cdot,\\cdot): \\mathbb{R}_{>0} \\times \\mathbb{R}_{>0} \\to "
"\\mathbb{R}_{>0}`: the Beta function"
msgstr ""

#: bayesml.bernoulli:21 of
#, python-brace-format
msgid ""
"p(\\theta) = \\mathrm{Beta}(\\theta|\\alpha_0,\\beta_0) = "
"\\frac{1}{B(\\alpha_0, \\beta_0)} \\theta^{\\alpha_0 - 1} "
"(1-\\theta)^{\\beta_0 - 1}.\n"
"\n"
msgstr ""

#: bayesml.bernoulli:24 of
#, python-brace-format
msgid ""
"\\mathbb{E}[\\theta] &= \\frac{\\alpha_0}{\\alpha_0 + \\beta_0}, \\\\\n"
"\\mathbb{V}[\\theta] &= \\frac{\\alpha_0 \\beta_0}{(\\alpha_0 + "
"\\beta_0)^2 (\\alpha_0 + \\beta_0 + 1)}.\n"
"\n"
msgstr ""

#: bayesml.bernoulli:28 of
msgid "The posterior distribution is as follows:"
msgstr ""

#: bayesml.bernoulli:30 of
#, python-brace-format
msgid ":math:`x^n = (x_1, x_2, \\dots , x_n) \\in \\{ 0, 1\\}^n`: given data"
msgstr ""

#: bayesml.bernoulli:31 of
#, python-brace-format
msgid ":math:`\\alpha_n \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.bernoulli:32 of
#, python-brace-format
msgid ":math:`\\beta_n \\in \\mathbb{R}_{>0}`: a hyperparameter"
msgstr ""

#: bayesml.bernoulli:34 of
#, python-brace-format
msgid ""
"p(\\theta | x^n) = \\mathrm{Beta}(\\theta|\\alpha_n,\\beta_n) = "
"\\frac{1}{B(\\alpha_n, \\beta_n)} \\theta^{\\alpha_n - 1} "
"(1-\\theta)^{\\beta_n - 1},\n"
"\n"
msgstr ""

#: bayesml.bernoulli:37 of
#, python-brace-format
msgid ""
"\\mathbb{E}[\\theta | x^n] &= \\frac{\\alpha_n}{\\alpha_n + \\beta_n}, "
"\\\\\n"
"\\mathbb{V}[\\theta | x^n] &= \\frac{\\alpha_n \\beta_n}{(\\alpha_n + "
"\\beta_n)^2 (\\alpha_n + \\beta_n + 1)},\n"
"\n"
msgstr ""

#: bayesml.bernoulli:41 of
msgid "where the updating rule of the hyperparameters is"
msgstr ""

#: bayesml.bernoulli:43 of
#, python-brace-format
msgid ""
"\\alpha_n = \\alpha_0 + \\sum_{i=1}^n I \\{ x_i = 1 \\},\\\\\n"
"\\beta_n = \\beta_0 + \\sum_{i=1}^n I \\{ x_i = 0 \\}.\n"
"\n"
msgstr ""

#: bayesml.bernoulli:47 of
msgid "The predictive distribution is as follows:"
msgstr ""

#: bayesml.bernoulli:49 of
#, python-brace-format
msgid ":math:`x_{n+1} \\in \\{ 0, 1\\}`: a new data point"
msgstr ""

#: bayesml.bernoulli:50 of
#, python-brace-format
msgid ":math:`\\alpha_\\mathrm{p} \\in \\mathbb{R}_{>0}`: a parameter"
msgstr ""

#: bayesml.bernoulli:51 of
#, python-brace-format
msgid ":math:`\\beta_\\mathrm{p} \\in \\mathbb{R}_{>0}`: a parameter"
msgstr ""

#: bayesml.bernoulli:52 of
#, python-brace-format
msgid ":math:`\\theta_\\mathrm{p} \\in [0,1]`: a parameter"
msgstr ""

#: bayesml.bernoulli:54 of
msgid ""
"p(x_{n+1} | x^n) = \\mathrm{Bern}(x_{n+1}|\\theta_\\mathrm{p}) "
"=\\theta_\\mathrm{p}^{x_{n+1}}(1-\\theta_\\mathrm{p})^{1-x_{n+1}},\n"
"\n"
msgstr ""

#: bayesml.bernoulli:57 of
#, python-brace-format
msgid ""
"\\mathbb{E}[x_{n+1} | x^n] &= \\theta_\\mathrm{p}, \\\\\n"
"\\mathbb{V}[x_{n+1} | x^n] &= \\theta_\\mathrm{p} (1 - "
"\\theta_\\mathrm{p}),\n"
"\n"
msgstr ""

#: bayesml.bernoulli:61 of
msgid ""
"where the parameters are obtained from the hyperparameters of the "
"posterior distribution as follows."
msgstr ""

#: bayesml.bernoulli:63 of
#, python-brace-format
msgid ""
"\\theta_\\mathrm{p} = \\frac{\\alpha_n}{\\alpha_n + \\beta_n}.\n"
"\n"
msgstr ""

